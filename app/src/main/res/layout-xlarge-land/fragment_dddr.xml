                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Refractory(ms)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
