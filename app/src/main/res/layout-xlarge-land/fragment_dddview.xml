<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".psa.FragmentPsaDdd">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_margin="2dp"
        android:gravity="center"
        android:background="@drawable/border">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">

            <CheckBox
                android:id="@+id/checkbox"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:text="Asynchronous"
                android:textSize="20sp"></CheckBox>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinMode"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtMode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Mode"
                    android:textSize="20sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinRate"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtRate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Rate(ppm)"
                    android:textSize="20sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinAvi"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtAvd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="AVD"
                    android:textSize="20sp"
                    />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/amp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:gravity="center">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_marginBottom="30dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <ImageButton
                        android:id="@+id/decrement_aAmp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/minus"
                        android:layout_marginRight="30dp"></ImageButton>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:layout_width="130dp"
                            android:layout_height="60dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/border">
                            <TextView
                                android:id="@+id/ampAtext"
                                android:layout_width="130dp"
                                android:layout_height="60dp"
                                android:gravity="center"
                                android:textSize="30sp"
                                android:background="@drawable/border"
                                ></TextView>

                        </RelativeLayout>
                        <TextView
                            android:id="@+id/txtAmpa"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/colorPrimary"
                            android:text="A Amplitude (V)"
                            android:textSize="15sp"
                            />
                    </RelativeLayout>
                    <ImageButton
                        android:id="@+id/increment_aAmp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/plus"
                        android:layout_marginLeft="30dp"></ImageButton>
                </LinearLayout>

                <!--     <LinearLayout
                         android:layout_width="wrap_content"
                         android:layout_height="40dp"
                         android:orientation="horizontal"
                         android:gravity="center"
                         android:visibility="invisible">
                         <ImageButton
                             android:id="@+id/decrement_aAmp"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/minus"></ImageButton>
                         <SeekBar
                             android:id="@+id/seekBar_ampA"
                             android:layout_width="300dp"
                             android:layout_height="wrap_content"
                             android:max="47"
                             android:visibility="invisible"
                             />
                         <ImageButton
                             android:id="@+id/increment_aAmp"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/plus"></ImageButton>
                     </LinearLayout>-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/setValuea1"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="3.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuea2"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="4.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuea3"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="5.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuea4"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="10.0"
                        ></Button>
                    <!--  <Button
                          android:id="@+id/setValuea5"
                          android:layout_width="50dp"
                          android:layout_height="wrap_content"
                          android:padding="0dp"
                          android:text="12.0"
                          android:layout_marginRight="30dp"
                          ></Button>
                      <Button
                          android:id="@+id/setValuea6"
                          android:layout_width="50dp"
                          android:layout_height="wrap_content"
                          android:padding="0dp"
                          android:text="15.0"
                          ></Button>-->

                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <ImageButton
                        android:id="@+id/decrement_vAmp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/minus"
                        android:layout_marginRight="30dp"></ImageButton>
                    <!--<SeekBar
                        android:id="@+id/seekBar_ampV"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:max="47"
                        />-->
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:layout_width="130dp"
                            android:layout_height="60dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/border">
                            <Spinner
                                android:id="@+id/spinAmp"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:spinnerMode="dropdown"
                                android:layout_marginLeft="15dp"
                                tools:ignore="SpeakableTextPresentCheck" />
                            <!-- <TextView
                                 android:id="@+id/ampVtext"
                                 android:layout_width="130dp"
                                 android:layout_height="60dp"
                                 android:layout_alignParentEnd="true"
                                 android:layout_alignParentBottom="true"
                                 android:layout_marginEnd="0dp"
                                 android:layout_marginBottom="0dp"
                                 android:background="@drawable/border"
                                 android:gravity="center"
                                 android:textSize="20sp"></TextView>-->

                        </RelativeLayout>
                        <TextView
                            android:id="@+id/txtVamp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:textColor="@color/colorPrimary"
                            android:layout_marginStart="10dp"
                            android:text="V Amplitude (V)"
                            android:textSize="15sp"
                            />
                    </RelativeLayout>
                    <ImageButton
                        android:id="@+id/increment_vAmp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/plus"
                        android:layout_marginLeft="30dp"></ImageButton>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/setValuev1"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="3.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuev2"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="4.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuev3"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="5.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuev4"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="10.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuev5"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="12.0"
                        android:layout_marginRight="30dp"
                        ></Button>
                    <Button
                        android:id="@+id/setValuev6"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:padding="0dp"
                        android:text="15.0"
                        ></Button>


                </LinearLayout>
            </LinearLayout>








        </LinearLayout>
        <LinearLayout
            android:id="@+id/parameter_a"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                >
                <RadioButton
                    android:id="@+id/redA"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:checked="true"
                    android:buttonTint="@color/CHERRY"
                    />
                <TextView
                    android:id="@+id/a"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="A"
                    android:textSize="30sp"
                    android:layout_marginLeft="20dp"
                    />
                <RadioButton
                    android:id="@+id/greenA"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:checked="true"
                    android:layout_marginLeft="35dp"
                    android:buttonTint="@color/colorPrimaryDark"
                    />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinSens_a"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtSens_a"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Sensitivity(mV)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinPw_a"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtPw_a"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="PW(ms)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="5dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinRef_a"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtRef_a"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Refractory(ms)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/parameter_v"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                >
                <RadioButton
                    android:id="@+id/redV"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:checked="true"
                    android:buttonTint="@color/CHERRY"
                    />
                <TextView
                    android:id="@+id/v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="V"
                    android:textSize="30sp"
                    android:layout_marginLeft="20dp"
                    />
                <RadioButton
                    android:id="@+id/greenV"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:checked="true"
                    android:layout_marginLeft="35dp"
                    android:buttonTint="@color/colorPrimaryDark"
                    />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinSens_v"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtSens_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Sensitivity(mV)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinPw_v"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtPw_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="PW(ms)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="5dp"
                    android:background="@drawable/border">

                    <Spinner
                        android:id="@+id/spinRef_v"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        tools:ignore="SpeakableTextPresentCheck" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/txtRef_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:background="#ffffff"
                    android:textColor="@color/colorPrimary"
                    android:text="Refractory(ms)"
                    android:textSize="15sp"
                    />
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>