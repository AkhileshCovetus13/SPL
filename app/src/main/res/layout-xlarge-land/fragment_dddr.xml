                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="150dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="5dp"
                    android:background="@drawable/border">

                    <Spinner
